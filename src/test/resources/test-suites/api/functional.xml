<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd" >
<suite name="API TestSuite">
  <listeners>
    <listener class-name="com.talendradar.listeners.AllureSuccessAndFailureTestListener"/>
  </listeners>

  <test name="Authentication Module Tests">
    <classes>
      <class name="com.talendradar.tests.api.auth.EmailPasswordLoginStrategyTests"/>
    </classes>
  </test>

  <test name="Assessment Module">
    <classes>
      <class name="com.talendradar.tests.api.assessment.CreateOrUpdateWithValidDataTests"/>
      <class name="com.talendradar.tests.api.assessment.CreateOrUpdateWithInvalidDataTests"/>
    </classes>
  </test>

  <test name="Feedback Module">
    <classes>
    </classes>
  </test>

  <test name="Session Module Management">
    <classes>
      <class name="com.talendradar.tests.api.admin.sessions.RetrieveSessionsTests"/>
      <class name="com.talendradar.tests.api.admin.sessions.RevokeSessionTests"/>
    </classes>
  </test>

  <test name="Registration Module">
    <classes>
      <class name="com.talendradar.tests.api.users.registration.RegisterUserTests"/>
      <class name="com.talendradar.tests.api.users.registration.CompleteRegistrationTests"/>
    </classes>
  </test>

  <test name="User Module">
    <classes>
      <class name="com.talendradar.tests.api.users.GetCurrentUserTests"/>
      <class name="com.talendradar.tests.api.users.GetUsersTests"/>
    </classes>
  </test>

  <test name="Other Modules">
    <classes>
      <class name="com.talendradar.tests.api.DimensionTests"/>
      <class name="com.talendradar.tests.api.roles.GetRolesTests"/>
    </classes>
  </test>
</suite>
