<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd" >
<suite name="API TestSuite">
  <listeners>
    <listener class-name="com.talendradar.listeners.AllureSuccessAndFailureTestListener"/>
  </listeners>

  <test name="Authentication Tests">
    <classes>
      <class name="com.talendradar.tests.api.auth.EmailPasswordLoginStrategyTests"/>
      <class name="com.talendradar.tests.api.users.registration.RegisterUserTests"/>
    </classes>
  </test>

  <test name="Self-Assessment Tests">
    <classes>
      <class name="com.talendradar.tests.api.assessment.CreateOrUpdateWithValidDataTests"/>
    </classes>
  </test>

  <test name="Feedback Tests">
    <classes>
    </classes>
  </test>

  <test name="User Management Tests">
    <classes>
      <class name="com.talendradar.tests.api.admin.sessions.RetrieveSessionsTests"/>
      <class name="com.talendradar.tests.api.admin.sessions.RevokeSessionTests"/>
    </classes>
  </test>
</suite>
