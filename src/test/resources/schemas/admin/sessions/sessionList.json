{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "required": ["success", "message", "data", "pageInfo"],
  "properties": {
    "success": {
      "type": "boolean",
      "const": true
    },
    "message": {
      "type": "string",
      "const": "Sessions fetched successfully"
    },
    "data": {
      "type": "array",
      "items": {
        "type": "object",
        "required": ["id", "user_id", "ip_address", "device_info", "status", "token", "timestamp"],
        "properties": {
          "id": {
            "type": "string",
            "format": "uuid"
          },
          "user_id": {
            "type": "string",
            "format": "uuid"
          },
          "ip_address": {
            "type": "string",
            "format": "ipv4"
          },
          "device_info": {
            "type": "string"
          },
          "status": {
            "type": "string",
            "enum": ["Active", "Expired"]
          },
          "token": { "type": "string" },
          "timestamp": {
            "type": "string",
            "format": "date-time"
          }
        },
        "additionalProperties": false
      }
    },
    "pageInfo": { "$ref": "../../common/pagination.json" },
    "errors": { "type": "null" }
  },
  "additionalProperties": false
}
