{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "required": [
    "id",
    "email",
    "status",
    "roleName"
  ],
  "properties": {
    "id": {
      "type": "string",
      "format": "uuid"
    },
    "email": {
      "type": "string",
      "format": "email"
    },
    "status": {
      "type": "string",
      "const": "ACTIVE"
    },
    "roleName": {
      "type": "string",
      "enum": [
        "ADMIN",
        "MANAGER",
        "DEVELOPER"
      ]
    }
  },
  "additionalProperties": false
}
